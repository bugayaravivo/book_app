<h1>Editing Book</h1>
<%= form_with model: @book, url: book_path(@book.id),method: :patch do |f| %>

  <p>title</p>
  <%= f.text_field :title %>

  <p>body</p>
  <%= f.text_area :body %>

  <%= f.submit 'Update Book' %>
  
<% end %>

<span><%= link_to "show",book_path(@book.id) %></span>
<span><%= link_to "back",books_path %></span>